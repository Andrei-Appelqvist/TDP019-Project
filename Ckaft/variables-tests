x = 1 + 1
print("1 + 1 ->", x)

c = 10 - 25
print("10 - 25 ->", c)

z = 2 + 2 * 3
print("2 + 2 * 3 ->", z)

v = 3 + 3 + 4/2
print("3 + 3 + 4/2 ->", v)

b = true or false
print("true or false ->", b)

n = true and false
print("true and false ->", n)

z = 4 <= 5
print("4 <= 5 ->", z)

d = 10 == 10
print("10 == 10 ->", d)
